<?xml version="1.0" encoding="utf-8" ?>
<!-- this is a data node for application wide settings --> 
<component name="AppSettingsNode" extends="Node">
    <interface>
        <!-- testing flags -->
        <field id="level_visible" type="bool" value="true" />
        <!-- tiles are squares -->
        <field id="tileset" type="string" value="tileset_default" />
        <field id="tile_size" type="integer" value="40" />
        <field id="room_min_width" type="integer" value="6" />
        <field id="room_min_height" type="integer" value="4" />
        <field id="room_max_width" type="integer" value="16" />
        <field id="room_max_height" type="integer" value="6" />
	</interface> 
</component>


